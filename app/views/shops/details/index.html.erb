<p id="notice"><%= notice %></p>

<h1>Details</h1>

<table>
  <thead>
    <tr>
      <th>Shop</th>
      <th>Open</th>
      <th>Holiday</th>
      <th>Access</th>
      <th>Parking</th>
      <th>Payment</th>
      <th>Room</th>
      <th>Smoking</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @details.each do |detail| %>
      <tr>
        <td><%= detail.shop %></td>
        <td><%= detail.open %></td>
        <td><%= detail.holiday %></td>
        <td><%= detail.access %></td>
        <td><%= detail.parking %></td>
        <td><%= detail.payment %></td>
        <td><%= detail.room %></td>
        <td><%= detail.smoking %></td>
        <td><%= link_to 'Show', detail %></td>
        <td><%= link_to 'Edit', edit_detail_path(detail) %></td>
        <td><%= link_to 'Destroy', detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
